{
  "name": "apish",
  "version": "1.0.1",
  "description": "Mock APIs and Services from API Blueprint or Swagger",
  "main": "dist/apish.js",
  "scripts": {
    "precommit": "npm test && npm run build",
    "prepublish": "npm test && npm run build",
    "build": "rimraf dist && webpack --no-minimize && webpack",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover node_modules/mocha/bin/_mocha --report lcovonly -- -R spec",
    "coveralls": "npm run coverage && coveralls < coverage/lcov.info && rimraf coverage",
    "test": "npm run test-server",
    "test-server": "mocha  --recursive --timeout 10000 --compilers js:babel-core/register --bail --require mocha.config.js",
    "tdd": "npm run test-server -- --watch"
  },
  "repository": "JackuB/apish",
  "keywords": [
    "api",
    "mock",
    "nock",
    "testing",
    "blueprint",
    "swagger",
    "apiary"
  ],
  "files": [
    "dist",
    "src"
  ],
  "author": {
    "name": "Jakub Mikulas",
    "email": "jakub@mikul.as"
  },
  "engines": {
    "node": ">=0.10.0",
    "npm": ">=2.13.0"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel": "^5.8.3",
    "babel-core": "^5.8.3",
    "babel-eslint": "^4.1.4",
    "babel-loader": "^5.3.3",
    "chai": "^3.2.0",
    "coveralls": "^2.11.4",
    "eslint": "^1.4.1",
    "eslint-plugin-babel": "^2.1.1",
    "hippie": "^0.4.0",
    "husky": "^0.10.1",
    "isparta": "^3.5.1",
    "mocha": "^2.3.3",
    "mocha-lcov-reporter": "^1.0.0",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.12.2",
    "bluebird": "^3.1.1",
    "deckardcain": "^0.3.2",
    "lodash": "^3.10.1",
    "lodash-api-description": "0.0.2",
    "nock": "^3.6.0",
    "request": "^2.67.0",
    "uri-template-lite": "^0.1.11",
    "urijs": "^1.17.0"
  }
}
